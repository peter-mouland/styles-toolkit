{
  "name": "styles-toolkit",
  "version": "0.4.2",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/notonthehighstreet/styles-toolkit.git"
  },
  "scripts": {
    "env": "env",
    "build": "cp -R src/assets dist && webpack",
    "prestart": "npm run build",
    "start": "node docs/app.js",
    "watch": "npm run build -- -w",
    "predeploy": "npm run build",
    "preversion": "npm run build",
    "postversion": "git push && git push --tags",
    "deploy": "./bin/deploy.js --v=$npm_package_version"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.7",
    "css-loader": "^0.23.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "node-sass": "^3.8.0",
    "postcss-loader": "^0.9.1",
    "s3": "^4.4.0",
    "sass-loader": "^4.0.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.1",
    "yargs": "^4.7.1"
  },
  "dependencies": {
    "express": "^4.13.4",
    "postcss-prefix-selector": "^1.4.0"
  }
}
